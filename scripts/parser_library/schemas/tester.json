{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"name": {
			"type": "string"
		},
		"time": {
			"allOf": [
				{
					"$ref": "definitions.json#/time"
				}
			],
			"properties": {
				"days": {},
				"start": {},
				"end": {}
			},
			"additionalProperties": false
		},
		"location": {
			"allOf": [
				{
					"$ref": "definitions.json#/location"
				}
			],
			"properties": {
				"school_code": {},
				"campus": {},
				"building": {},
				"room": {},
				"building_room": {}
			},
			"additionalProperties": false
		}
	},
	"additionalProperties": false,
	"required": [
		"name"
	]
}